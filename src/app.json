{
  "pages": ["pages/index/index", "pages/login/index", "pages/automation/index", "pages/mine/index"],
  "subpackages": [
    {
      "root": "package-distribution",
      "pages": [
        "scan/index",
        "link-gateway/index",
        "wifi-connect/index",
        "add-subdevice/index",
        "search-subdevice/index",
        "bind-home/index",
        "change-wifi-success/index",
        "choose-device/index",
        "choose-sensor/index",
        "choose-light/index",
        "connect-guide/index"
      ]
    },
    {
      "root": "package-mine",
      "pages": [
        "project-manage/index",
        "project-transfer/index",
        "space-manage/index",
        "space-detail/index",
        "member-manage/index",
        "device-replace/index",
        "ota/index",
        "setting/index",
        "device-manage/index",
        "device-manage/device-detail/index",
        "device-manage/group-detail/index",
        "voice-control/index",
        "guideline/index",
        "device-category/index",
        "help/list/index",
        "help/show/index",
        "help/webview/index"
      ]
    },
    {
      "root": "package-space-control",
      "pages": [
        "index/index",
        "space-list/index",
        "scene-edit/index",
        "scene-request-list/index",
        "scene-list/index",
        "group/index"
      ]
    },
    {
      "root": "package-protocol",
      "pages": ["protocol-show/index", "protocol-list/index", "add-virtual-device/index"]
    },
    {
      "root": "package-auth",
      "pages": [
        "pages/auth-screen/index",
        "pages/index/index",
        "pages/meiju/index",
        "pages/project-list/index",
        "pages/confirm-auth/index",
        "pages/device-list/index"
      ]
    },
    {
      "root": "package-automation",
      "pages": [
        "automation/index",
        "automation-log/index",
        "automation-add/index",
        "automation-edit-yijian/index",
        "scene-request-list-yijian/index",
        "scene-success/index"
      ]
    }
  ],
  "preloadRule": {
    "pages/index/index": {
      "network": "all",
      "packages": ["package-space-control", "package-distribution", "package-mine", "package-automation"]
    },
    "package-distribution/choose-device/index": {
      "network": "all",
      "packages": ["package-auth"]
    }
  },
  "tabBar": {
    "custom": true,
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页"
      },
      {
        "pagePath": "pages/automation/index",
        "text": "智能场景"
      },
      {
        "pagePath": "pages/mine/index",
        "text": "我的"
      }
    ]
  },
  "lazyCodeLoading": "requiredComponents",
  "usingComponents": {
    "van-button": "@vant/weapp/button/index",
    "van-overlay": "@vant/weapp/overlay/index",
    "van-cell": "@vant/weapp/cell/index",
    "van-slider": "@vant/weapp/slider/index",
    "van-popup": "@vant/weapp/popup/index",
    "van-cell-group": "@vant/weapp/cell-group/index",
    "van-dialog": "@vant/weapp/dialog/index",
    "van-toast": "@vant/weapp/toast/index",
    "nav-bar": "./components/custom-nav-bar/index",
    "project-select-menu": "./components/project-select-menu/index",
    "add-space": "./components/add-space/index",
    "svg-icon": "./components/svg-icon/index",
    "van-field": "@vant/weapp/field/index",
    "device-card": "./components/device-card/index"
  },
  "window": {
    "navigationStyle": "custom",
    "backgroundTextStyle": "light",
    "backgroundColor": "#eef2f7",
    "navigationBarTextStyle": "black"
  },
  "sitemapLocation": "sitemap.json",
  "networkTimeout": {
    "request": 15000,
    "downloadFile": 10000
  },
  "debug": true,
  "permission": {
    "scope.userLocation": {
      "desc": "本应用访问位置信息，用于发现附近设备及WLAN列表等功能"
    },
    "scope.userFuzzyLocation": {
      "desc": "本应用访问位置信息，用于获取天气信息提示等功能"
    }
  },
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true
    }
  }
}
