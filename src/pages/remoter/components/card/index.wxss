.card-wrapper {
  height: 176rpx;
  width: 700rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  border-radius: 32rpx;
  margin-top: 40rpx;
  margin-left: 25rpx;
  padding: 0 40rpx;
  background-image: linear-gradient(180deg, #f2f5fc 10%, #ffffff 74%);
  border: 2rpx solid #fff;
  box-sizing: border-box;
}
.card-wrapper:active {
  background-image: none;
  background-color: #f6f6f6;
}
