<template name="title">
  <text class="h-50rpx text-36rpx text-black-2a mt-48rpx font-medium">{{title}}</text>
</template>
<template name="bottom-btn">
  <view class="h-96rpx"></view>

  <view class="flex gap-46rpx justify-center w-686rpx">
    <van-button type="primary" class="grow" size="large" disabled="{{disabled}}" bind:click="handleConfirm" custom-class="btn-class-fix">{{ confirmText || '确定'}}</van-button>
  </view>

  <view class="h-96rpx"></view>
</template>
