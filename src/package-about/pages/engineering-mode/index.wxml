<view class="h-100vh bg-hex-eef2f7">
  <nav-bar
    title="工程模式"
    left-arrow
    bind:leftTap="goBack"
  ></nav-bar>

  <view class="h-24rpx"></view>

  <van-cell-group inset>
    <van-cell
      title="局域网模式"
      center
      label="局域网适用于私有化部署工程"
      border="{{false}}"
    >
      <van-switch
        slot="right-icon"
        checked="{{ isLanOn }}"
        bind:change="onChangeLanSwitch"
      />
    </van-cell>
  </van-cell-group>
</view>

<van-toast id="van-toast" />

<van-dialog id="van-dialog" />
