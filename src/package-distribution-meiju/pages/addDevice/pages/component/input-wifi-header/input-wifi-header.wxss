.header {
  height: 60rpx;
  width: 100%;
  padding-left: 32rpx;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 60rpx;
  font-size: 18px;
  white-space: nowrap;
  position: fixed;
  top: 44rpx;
  left: 0;
  z-index: 99;
  display: flex;
  align-items: center;
}

.back {
  width: 70rpx;
  height: 70rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.back-img {
  width: 34rpx;
  height: 34rpx;
  display: inline-block;
}
