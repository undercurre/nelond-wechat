.foundAp {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 14rpx 32rpx 0;
}

.foundAp-text {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #333333;
  letter-spacing: 0;
  line-height: 40rpx;
}

.foundAp-img {
  margin-top: 24rpx;
  width: 686rpx;
  height: 360rpx;
  background: #f6f6f6;
  border-radius: 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.foundAp-img-image {
  width: 400rpx;
  height: 360rpx;
}

.foundAp-btn {
  margin-top: 60rpx;
  width: 686rpx;
  height: 92rpx;
  background: #488fff;
  border-radius: 48rpx;
  font-size: 32rpx;
  color: #ffffff;
  text-align: center;
  line-height: 92rpx;
  font-weight: 500;
}

.foundAp-btn-colmo {
  margin-top: 48rpx;
  width: 686rpx;
  height: 92rpx;
  background: #af6437;
  font-size: 32rpx;
  color: #ffffff;
  text-align: center;
  line-height: 92rpx;
  font-weight: 400;
}

.popup-title {
  font-size: 36rpx;
  color: #27282a;
  text-align: center;
  font-weight: 500;
  line-height: 50rpx;
  margin: 50rpx auto 0;
}

.step {
  font-size: 28rpx;
  color: #27282a;
  line-height: 40rpx;
  font-weight: 400;
  margin-top: 48rpx;
  padding: 0 40rpx;
  box-sizing: border-box;
}

.btn {
  color: #488fff;
}

.bottom {
  height: 100rpx;
}

.tip {
  margin-top: 36rpx;
  margin-bottom: 80rpx;
  font-size: 28rpx;
  color: #232323;
  text-align: center;
  line-height: 28rpx;
  font-weight: 400;
}

.cancel,
.confirm {
  width: 328rpx;
  height: 92rpx;
  line-height: 92rpx;
  background: #e9f1ff;
  border-radius: 48px;
  font-size: 32rpx;
  text-align: center;
}

.wifiSetting {
  display: flex;
  align-items: center;
  justify-content: center;
}

.wifiView {
  height: 360rpx;
  width: 686rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #f6f6f6;
  border-radius: 24px;
  margin-top: 32rpx;
}

.wifi-img {
  width: 400rpx;
  height: 360rpx;
}

.colmo-style {
  display: flex;
  justify-content: flex-start;
}

.num {
  min-width: 36rpx;
  max-height: 36rpx;
  background-color: rgba(255, 255, 255, 0.1);
  border-radius: 50%;
  margin-right: 22rpx;
  font-size: 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.colmo-style-step {
  display: flex;
  align-items: baseline;
}
