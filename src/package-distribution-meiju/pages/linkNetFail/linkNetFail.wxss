.page-container {
  padding-bottom: 184rpx;
}

.flex-c {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.link-flow {
  width: 100%;
  margin: 112rpx auto 48rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.fail-image {
  margin: 0rpx auto 32rpx;
  display: block;
  width: 216rpx;
  height: 216rpx;
}

.fail-title {
  font-size: 48rpx;
  /* color: #FF9500; */
  color: #000000;
  letter-spacing: 0;
  text-align: center;
  line-height: 48rpx;
  font-weight: 500;
}

.fail-code {
  margin-top: 16rpx;
  width: 80%;
  line-height: 1.4;
  font-size: 32rpx;
  color: #8a8a8f;
  letter-spacing: 0;
  text-align: center;
  font-weight: 400;
}

.r {
  display: flex;
  align-items: flex-start;
  line-height: 64rpx;
}

.num {
  width: 36rpx;
  height: 36rpx;
  line-height: 36rpx;
  text-align: center;
  margin-top: 16rpx;
  margin-right: 16rpx;
  border-radius: 50%;
  color: #458bff;
  background: #e9f1ff;
  font-size: 24rpx;
}

.text {
  width: 90%;
  font-size: 32rpx;
}

.tip {
  display: flex;
  align-items: center;
}

.tips {
  width: 685rpx;
  display: flex;
  align-items: top;
  margin: 30rpx auto;
  padding-left: 30rpx;
}

.tip-text {
  display: inline-block;
  width: 90%;
  text-align: left;
  font-size: 12px;
  line-height: 1.4;
  color: #ff8225;
  vertical-align: top;
}

.switch-wifi {
  z-index: 100;
  font-size: 14px;
  color: #488fff;
  line-height: 24px;
  position: absolute;
  top: 50%;
  right: 20px;
  transform: translateY(-50%);
}

.show-psw {
  z-index: 100;
  width: 24px;
  height: 24px;
  position: absolute;
  top: 50%;
  right: 20px;
  transform: translateY(-50%);
}

.input-area {
  position: relative;
}

.tip-list {
  width: 88%;
  margin: 24rpx auto 24rpx;
  font-size: 24rpx;
  color: #666666;
  text-align: left;
  line-height: 1.4;
  vertical-align: top;
}

.tip2 {
  margin-top: 28rpx;
}
.q-mark {
  width: 32rpx;
  height: 32rpx;
  margin-left: 8rpx;
  background-color: #c7c7cc;
  border-radius: 50%;
  color: #fff;
  text-align: center;
  line-height: 32rpx;
}

.circle {
  display: inline-block;
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  background-color: #666666;
  margin-right: 12rpx;
  vertical-align: middle;
}

.text {
  vertical-align: middle;
}

.retry {
  width: 91.5%;
  height: 92rpx;
  margin: 8vh auto 3vh;
  line-height: 92rpx;
  text-align: center;
  border-radius: 48rpx;
  background: #488fff;
  color: #fff;
}

.back-to-index {
  font-size: 16px;
  color: #488fff;
  line-height: 16px;
  font-weight: 500;
  text-align: center;
}

.spaceTips {
  width: 685rpx;
  display: flex;
  align-items: top;
  margin: 30rpx auto;
  padding-left: 30rpx;
}
