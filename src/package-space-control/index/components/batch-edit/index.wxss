.popup-shadow {
  box-shadow: 0px -4px 32px 0px rgba(97, 109, 136, 0.1);
}
.bg-select {
  background-image: linear-gradient(155deg, #468cfb 6%, #6ba3fc 96%);
}

.top-transform {
  transform: translateY(-100%);
}

.bottom-transform {
  transform: translateY(100%);
}

.edit-box-height {
  height: calc(env(safe-area-inset-bottom) + 298rpx);
}
